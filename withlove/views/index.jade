extends layout

block title
  title= title

block styles
  link(href="/external/bootstrap-3.3.5-dist/css/bootstrap.min.css" rel="stylesheet")
  link(rel='stylesheet', href='/stylesheets/style.css')


block scripts
  script(src="/external/jquery-1.11.3.min.js")
  script(src="/external/bootstrap-3.3.5-dist/js/bootstrap.min.js")
